{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "aspect-ratio",
          "width: fit-content",
          "min-height: fit-content"
        ]
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "button-type": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/name-role-value": [
      "default",
      {
        "input-button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "definition-list": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "disown-opener": "off"
  }
}